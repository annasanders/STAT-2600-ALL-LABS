countries <- function(country1, country2, yr) {
x <- gapminder%>%
filter(country== country1, year== yr)%>%
select(lifeExp)
y <- gapminder%>%
filter(country== country2, year== yr)%>%
select(lifeExp)
if(as.numeric(x)> as.numeric(y)) {
diff<- x-y
} else(as.numeric(y)> as.numeric(x)) {
diff<- y-x
}
return(diff)
}
