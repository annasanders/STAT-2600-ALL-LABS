head(milk)
ggplot(data=bday_state, aes(x=state, fill=state, y=milk_million))+
geom_col()+
coord_flip()+
theme(legend.position = "none")+
labs(y = "Milk Produciton (Millions of Gallons", x= "State", title= "Milk Production by State in 1999")
